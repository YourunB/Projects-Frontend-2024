{
  "name": "rss-puzzle",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.base.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "lint": "eslint **/*.ts",
    "format": "npx prettier . --write",
    "ci:format": "npx prettier . --check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check .",
      "pre-push": "eslint . && validate-branch-name"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "validate-branch-name": {
    "pattern": "feat/RSS-PZ-*",
    "errorMsg": "Error branch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Yury Butskevich",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "url-loader": "^4.1.1"
  }
}
